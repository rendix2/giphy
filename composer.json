{
  "name": "rendix2/giphy",
  "description": "PHP Giphy.com API",
  "keywords": [
    "PHP",
    "Giphy.com",
    "API"
  ],
  "type": "library",
  "license": [
    "MIT",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "require": {
    "php": ">= 8.1.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Rendix2\\Giphy\\": "src"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/thanks": false
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "lock": true
  }
}
